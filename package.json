{
  "name": "Dungeon",
  "version": "1.0.9",
  "description": "Procedural explorable dungeons",
  "keywords": [
    "express",
    "sql",
    "knex",
    "typescript",
    "objection",
    "rest",
    "api",
    "threejs",
    "mobx",
    "FMOD"
  ],
  "main": "dist/server",
  "scripts": {
    "docker": "docker-compose up",
    "devserver": "clear ; nodemon --watch",
    "dev": "clear; webpack --watch",
    "build": "webpack",
    "start": "dist/server",
    "clean": "rm -rf dist",
    "push": "git add -A; git commit -m 'v'; npm version patch; git push origin dungeon",
    "init": "knex --knexfile ./src/knexfile.ts migrate:latest ",
    "lint": "tslint 'src/**/*.ts?(x)' -t verbose",
    "test": "mocha test/**/**.spec.js"
  },
  "nodemonConfig": {
    "ignore": [
      ".git",
      "node_modules/**/node_modules",
      "src",
      "static",
      "binsave"
    ],
    "verbose": true,
    "watch": [
      "./dist/**/*"
    ]
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "dependencies": {
    "ajv": "6.6.1",
    "body-parser": "1.18.3",
    "cannon": "github:schteppe/cannon.js",
    "compression": "1.7.3",
    "connect-redis": "^3.4.0",
    "cors": "2.8.5",
    "css-loader": "1.0.1",
    "es6-tween": "^5.3.1",
    "event-stream": "3.3.4",
    "express": "4.16.4",
    "express-session": "1.15.6",
    "gsap": "^2.0.2",
    "helmet": "3.15.0",
    "http": "0.0.0",
    "knex": "0.15.2",
    "knex-db-manager": "0.4.0",
    "mobx": "5.6.0",
    "mobx-react": "5.4.2",
    "mobx-react-devtools": "6.0.3",
    "morgan": "1.9.1",
    "multer": "1.4.1",
    "mysql": "2.16.0",
    "objection": "1.4.0",
    "ramda": "0.26.1",
    "random-seed": "0.3.0",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "three": "0.99.0",
    "three-gltf-loader": "^1.99.0",
    "tslint": "5.11.0",
    "tsutils": "3.5.2",
    "typescript": "3.2.1"
  },
  "devDependencies": {
    "@types/cannon": "0.1.3",
    "@types/connect-redis": "0.0.8",
    "@types/express": "4.16.0",
    "@types/gsap": "^1.20.0",
    "@types/knex": "0.15.1",
    "@types/multer": "1.3.7",
    "@types/node": "10.12.11",
    "@types/react": "16.7.12",
    "@types/three": "0.93.10",
    "chai": "4.2.0",
    "chai-http": "4.2.0",
    "colors": "1.3.2",
    "eslint": "5.9.0",
    "mini-css-extract-plugin": "0.4.5",
    "mocha": "5.2.0",
    "node-sass": "4.10.0",
    "nodemon": "1.18.7",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "ts-loader": "5.3.1",
    "ts-node": "7.0.1",
    "ts-node-dev": "^1.0.0-pre.30",
    "tsc-watch": "1.0.31",
    "tslint-config-airbnb": "5.11.1",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "1.7.2"
  }
}
