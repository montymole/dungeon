{
  "name": "Dungeon",
  "version": "1.0.11",
  "description": "Procedural explorable dungeons",
  "keywords": [
    "express",
    "sql",
    "knex",
    "typescript",
    "objection",
    "rest",
    "api",
    "threejs",
    "mobx",
    "FMOD"
  ],
  "main": "dist/server",
  "scripts": {
    "docker:build": "docker-compose build",
    "docker:prod": "docker-compose up -d",
    "docker:dev": "docker-compose -f docker-compose-dev.yml up -d",
    "devserver": "clear ; nodemon --watch",
    "dev": "clear; webpack --watch",
    "build": "webpack",
    "start": "dist/server",
    "clean": "rm -rf dist",
    "push": "git add -A; git commit -m 'v'; npm version patch; git push origin dungeon",
    "init": "knex --knexfile ./src/knexfile.ts migrate:latest ",
    "lint": "tslint 'src/**/*.ts?(x)' -t verbose",
    "test": "mocha test/**/**.spec.js"
  },
  "nodemonConfig": {
    "ignore": [
      ".git",
      "node_modules/**/node_modules",
      "src",
      "static",
      "binsave"
    ],
    "verbose": true,
    "watch": [
      "./dist/**/*"
    ]
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "dependencies": {
    "body-parser": "1.19.0",
    "busboy": "^0.3.1",
    "cannon": "github:schteppe/cannon.js",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "css-loader": "3.1.0",
    "easystarjs": "^0.4.3",
    "es6-tween": "^5.5.10",
    "express": "4.17.1",
    "gsap": "^2.1.3",
    "helmet": "3.20.0",
    "http": "0.0.0",
    "knex": "0.19.1",
    "knex-db-manager": "0.5.0",
    "l1-path-finder": "^1.0.0",
    "mobx": "5.13.0",
    "mobx-react": "6.1.1",
    "mobx-react-devtools": "6.1.1",
    "mysql": "2.17.1",
    "ndarray": "^1.0.18",
    "objection": "1.6.9",
    "ramda": "0.26.1",
    "random-seed": "0.3.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "three": "0.107.00",
    "three-gltf-loader": "^1.106.00"
  },
  "devDependencies": {
    "@types/cannon": "0.1.4",
    "@types/connect-redis": "0.0.8",
    "@types/express": "4.17.0",
    "@types/gsap": "^1.20.2",
    "@types/knex": "0.16.1",
    "@types/multer": "1.3.7",
    "@types/node": "12.6.8",
    "@types/react": "16.8.23",
    "chai": "4.2.0",
    "chai-http": "4.3.0",
    "colors": "1.3.3",
    "eslint": "6.1.0",
    "mini-css-extract-plugin": "0.8.0",
    "mocha": "6.2.0",
    "node-sass": "4.12.0",
    "nodemon": "1.19.1",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "ts-loader": "6.0.4",
    "ts-node": "8.3.0",
    "ts-node-dev": "^1.0.0-pre.40",
    "tsc-watch": "2.2.1",
    "tslint": "5.18.0",
    "tslint-config-airbnb": "5.11.1",
    "tsutils": "3.14.1",
    "typescript": "3.5.3",
    "typescript-tslint-plugin": "^0.5.4",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6",
    "webpack-node-externals": "1.7.2"
  }
}
