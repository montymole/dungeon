{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "trailing-comma": false,
    "no-increment-decrement": false,
    "function-name": false,
    "strict-boolean-expressions": false,
    "max-line-length": [
      true,
      350
    ],
    "indent": [
      true,
      "spaces",
      2
    ],
    "semicolon": [
      true,
      "always"
    ],
    "space-before-function-paren": false,
    "member-ordering": false,
    "variable-name": false,
    "quotemark": [
      true,
      "single",
      "avoid-escape",
      "avoid-template"
    ],
    "member-access": [
      true,
      "no-public"
    ]
  },
  "rulesDirectory": []
}